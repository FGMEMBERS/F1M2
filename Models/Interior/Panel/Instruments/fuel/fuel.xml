<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>fuel.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <object-name>knob</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <factor-prop>/sim/model//material/instruments/factor</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>/consumables/fuel/tank[0]/level-gal_us</property>
    <interpolation>
      <entry><ind>    0 </ind><dep>   0 </dep></entry>
      <entry><ind>  2.642 </ind><dep>  60 </dep></entry>
      <entry><ind>  5.284 </ind><dep>  90 </dep></entry>
      <entry><ind>  7.925 </ind><dep> 120 </dep></entry>
      <entry><ind> 10.57 </ind><dep> 150 </dep></entry>
      <entry><ind> 13.21 </ind><dep> 180 </dep></entry>
      <entry><ind> 15.85 </ind><dep> 270 </dep></entry>
      <entry><ind> 80.3 </ind><dep> 271 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.000 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.000 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
